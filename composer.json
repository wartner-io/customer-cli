{
    "name": "wartner-io/customer-cli",
    "description": "The customer cli for the wartner-io platform",
    "keywords": ["cli", "customers"],
    "homepage": "https://wartner.io",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/wartner-io/customer-cli/issues",
        "source": "https://github.com/wartner-io/customer-cli"
    },
    "authors": [{
        "name": "Florian Wartner",
        "email": "florian@wattner.io"
    }],
    "require": {
        "php": "^8.0",
        "laravel-zero/framework": "^9.0",
        "nunomaduro/laravel-console-menu": "^3.2",
        "nunomaduro/termwind": "^1.3",
        "symfony/console": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["customer"]
}
